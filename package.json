{
  "name": "@manuscripts/manuscript-transform",
  "description": "ProseMirror transformer for Manuscripts applications",
  "version": "0.49.0",
  "repository": "gitlab:mpapp-public/manuscripts-manuscript-transform",
  "license": "Apache-2.0",
  "main": "dist/cjs",
  "module": "dist/es",
  "types": "dist/types",
  "files": [
    "dist"
  ],
  "keywords": [
    "prosemirror"
  ],
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:cjs": "tsc --outDir dist/cjs --module commonjs --project tsconfig.build.json",
    "build:es": "tsc --outDir dist/es --declarationDir dist/types --declaration --project tsconfig.build.json",
    "dev": "npm-run-all --parallel \"build:* --watch\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "prebuild": "rimraf dist",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\" \"*.{js,json}\"",
    "preversion": "npm-run-all --parallel typecheck lint test",
    "test": "jest --runInBand",
    "test:debug": "DEBUG=manuscripts-transform yarn test",
    "typecheck": "tsc --noEmit",
    "version": "yarn build"
  },
  "dependencies": {
    "@manuscripts/data": "^0.1.3",
    "@manuscripts/library": "^0.6.2",
    "@manuscripts/manuscripts-json-schema": "^1.49.11",
    "debug": "^4.1.1",
    "mathjax-full": "^3.1.2",
    "mime": "^2.4.6",
    "prosemirror-model": "~1.14.0",
    "uuid": "^8.2.0",
    "w3c-xmlserializer": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.10.4",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@jats4r/dtds": "^0.0.7",
    "@manuscripts/eslint-config": "^0.2.2",
    "@manuscripts/examples": "^0.0.8",
    "@manuscripts/publish": "^0.2.2",
    "@manuscripts/rxdb": "8.2.0-manuscripts1",
    "@types/debug": "^4.1.4",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^24.9.1",
    "@types/lodash.pickby": "^4.6.6",
    "@types/mime": "^2.0.3",
    "@types/node": "^14.14.37",
    "@types/prosemirror-model": "~1.13.0",
    "@types/prosemirror-state": "~1.2.6",
    "@types/prosemirror-view": "~1.17.1",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "eslint": "^7.3.1",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "fs-extra": "^9.0.1",
    "husky": "^4.2.3",
    "jest": "^26.1.0",
    "jest-junit": "^10.0.0",
    "jszip": "^3.5.0",
    "libxmljs2": "^0.29.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "prosemirror-state": "~1.3.4",
    "prosemirror-view": "~1.18.2",
    "rimraf": "^2.6.3",
    "typescript": "^3.9.5"
  },
  "resolutions": {
    "@types/prosemirror-model": "~1.13.0",
    "random-uuid-v4": "^0.0.8"
  }
}
